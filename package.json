{
  "homepage": "https://mg901.github.io/effector-react-realworld-example-app",
  "name": "effector-react-realworld-example-app",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "predeploy": "npm run build",
    "start": "npm run clean && cross-env parcel src/index.html  --port 4100 --out-dir public",
    "build": "npm run clean && cross-env cross-env NODE_ENV=production parcel build src/index.html --no-source-maps --out-dir public --public-url /",
    "clean": "cross-env rimraf public .cache",
    "commit": "cross-env git-cz",
    "lint:file": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:all": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix.",
    "pretty": "cross-env fixme=fixme prettier --write '**/*.{j,t}{s,sx}'",
    "pretty:watch": "cross-env fixme=fixme onchange '**/*.{j,t}{s,sx}' -- prettier --write {{changed}}",
    "ts-check": "cross-env tsc",
    "ts-check:watch": "yarn ts-check -- --watch",
    "test": "cross-env jest",
    "test:watch": "cross-env jest --watch",
    "demo": "npx serve -s ./public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.0.4",
    "@testing-library/react-hooks": "^3.4.2",
    "@types/history": "4.7.7",
    "@types/jest": "^26.0.14",
    "@types/marked": "^1.1.0",
    "@types/node": "^14.11.2",
    "@types/react": "^16.9.50",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.3.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "commitizen": "^4.2.1",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-import-resolver-babel-module": "^5.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.3",
    "eslint-plugin-react-hooks": "^4.1.2",
    "gh-pages": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^26.5.0",
    "lint-staged": "^10.4.0",
    "node-static": "^0.7.11",
    "onchange": "^7.0.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.1.2",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.3"
  },
  "dependencies": {
    "@hot-loader/react-dom": "16.13.0",
    "api": "^2.3.1",
    "axios": "^0.20.0",
    "config": "^3.3.2",
    "effector": "^21.4.0",
    "effector-forms": "^0.0.14",
    "effector-react": "^21.0.6",
    "effector-storage": "^3.0.0",
    "features": "^0.1.0",
    "history": "4.10.1",
    "markdown-to-jsx": "^7.0.0",
    "patronum": "^0.108.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.13.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "ui": "^0.2.4"
  }
}
