{
  "homepage": "http://mg901.github.io/effector-react-realworld-example-app",
  "name": "effector-react-realworld-example-app",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "predeploy": "npm run build",
    "deploy": "cross-env gh-pages -d build",
    "start": "npm run clean && cross-env parcel src/index.html --out-dir public",
    "build": "npm run clean && cross-env cross-env parcel build src/index.html --out-dir public --public-url /",
    "clean": "cross-env rimraf public .cache",
    "commit": "cross-env git-cz",
    "lint:file": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:all": "cross-env eslint . --ext .js,.jsx,.ts,.tsx --fix.",
    "pretty": "cross-env fixme=fixme prettier --write '**/*.{j,t}{s,sx}'",
    "pretty:watch": "cross-env fixme=fixme onchange '**/*.{j,t}{s,sx}' -- prettier --write {{changed}}",
    "ts-check": "cross-env tsc",
    "ts-check:watch": "yarn ts-check -- --watch",
    "test": "cross-env jest",
    "test:watch": "cross-env jest --watch",
    "demo": "cross-env static public"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "effector": "^20.15.9",
    "effector-react": "^20.7.3",
    "effector-storage": "^2.0.0",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "use-onclickoutside": "^0.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@babel/preset-typescript": "^7.10.1",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@testing-library/react-hooks": "^3.3.0",
    "@types/history": "^4.7.6",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.9",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-config": "^5.0.1",
    "@types/react-router-dom": "^5.1.5",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "@typographist/postcss": "^6.1.1",
    "commitizen": "^4.1.2",
    "cross-env": "^7.0.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "gh-pages": "^3.0.0",
    "husky": "^4.2.5",
    "jest": "^26.0.1",
    "lint-staged": "^10.2.9",
    "node-static": "^0.7.11",
    "onchange": "^7.0.2",
    "parcel": "^1.12.4",
    "parcel-bundler": "^1.12.4",
    "postcss-nested": "^4.2.1",
    "postcss-normalize": "^9.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.5"
  }
}
